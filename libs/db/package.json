  "version": "0.0.1",
  "peerDependencies": {
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@oget/type": "workspace:^",
    "better-sqlite3": "^11.3.0",
    "pg": "^8.13.0",
