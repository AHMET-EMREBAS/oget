{
  "name": "@oget/common",
  "version": "0.0.1",
  "dependencies": {
    "@nestjs/common": "^10.4.5",
    "@nestjs/core": "^10.4.5",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "name": "common",
    "targets": {
      "test": {
        "executor": "@nx/vite:test",
        "outputs": [
          "{options.reportsDirectory}"
        ],
        "options": {
          "reportsDirectory": "../../coverage/libs/common"
        }
      }
    }
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0"
  }
}
